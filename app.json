{
  "expo": {
    "name": "learn-by-video",
    "slug": "learn-by-video",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "learnbyvideo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "extra": {
      "googleClientIdWeb": "977010988808-33abcmhc07rtaaujjn297ijdskf9a0e5.apps.googleusercontent.com",
      "googleClientIdIos": "977010988808-q4m4n0v9jrrh4t7sgok6luh4dv4ecotp.apps.googleusercontent.com",
      "googleClientIdAndroid": "977010988808-vuv8uhp01s7pt32v0uqe1mt0c5iichcf.apps.googleusercontent.com"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.learnbyvideo.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "package": "com.learnbyvideo.app"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      "expo-video",
      "expo-audio",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
